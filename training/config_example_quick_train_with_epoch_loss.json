{
  "model": {
    "alpha": 1.0,
    "weights_path": null,
    "backbone": {
      "name": "efficientnet_b0",
      "pretrained_weights": "DEFAULT"
    },
    "head": {
      "filters_size": [
        128,
        64,
        32
      ]
    }
  },
  "train": {
    "is_overfit": false,
    "lr": 5e-2,
    "batch_size": 32,
    "subset_len": 128,
    "val_subset_len": 96,
    "calculate_epoch_loss": true,
    "lr_schedule": {
      "patience": 10,
      "factor": 0.2,
      "min_lr": 1e-5
    },
    "stop_criteria": {
      "loss": 1,
      "epoch": 50
    }
  },
  "data": {
    "image_set": "val",
    "is_download": false,
    "class_amount": 20,
    "images_folder": "../VOC/VOCdevkit/VOC2007/JPEGImages",
    "ann_file": "../VOC_COCO/pascal_trainval2007.json"
  }
}
